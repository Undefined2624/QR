@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - QRMascotas</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/QrMascotas.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/QRMascotas.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="container-fluid mt-2">
            <a class="navbar-brand" href="#">
                <img src="~/Images/png.png" alt="" width="60" height="60" class="d-inline-block align-text-center">
                PukiScan
            </a>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">

            <div class="d-flex justify-content-center align-items-center text-white">
                <h1>Tom</h1>
            </div>
            <div class="d-flex align-items-center justify-content-center">
                <img src="/Images/Tom.jpg" class="img-fluid img-thumbnail h-auto" alt="Tom" style="width:15%; top:18%">
            </div>

            <div class="Contenido w-100 h-75">
                <div class="p-3 mt-3">
                    <nav>
                        <div class="nav nav-tabs bg-warning" id="nav-tab" role="tablist">
                            <a class="nav-item nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">Mis datos</a>
                            <a class="nav-item nav-link text-white" id="nav-profile-tab" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">Salud</a>
                            <a class="nav-item nav-link text-white" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Importante</a>
                        </div>
                    </nav>
                    <div class="tab-content bg-white rounded-2" id="nav-tabContent">
                        <div class="tab-pane fade show active p-3" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="d-flex">
                                <div class="row w-100">
                                    <div class="col-6">
                                        <div class="table table-responsive table-borderless">
                                            <table>
                                                <tr><p class="fw-bolder fs-5">Sobre mí</p></tr>
                                                <tr>
                                                    <td>Especie:</td>
                                                    <td>gato</td>
                                                </tr>
                                                <tr>
                                                    <td>Raza:</td>
                                                    <td>DHL</td>
                                                </tr>
                                                <tr>
                                                    <td>Color:</td>
                                                    <td>Blanco con plomo atigrado</td>
                                                </tr>
                                                <tr>
                                                    <td>Género:</td>
                                                    <td>Macho</td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="table table-responsive table-borderless">
                                            <table>
                                                <tr><p class="fw-bolder fs-5">Contacto</p></tr>
                                                <tr>
                                                    <td>Mis dueños:</td>
                                                    <td> Jorge</td>
                                                </tr>
                                                <tr>
                                                    <td>Número de contacto:</td>
                                                    <td>+56955297032</td>
                                                </tr>
                                                <tr>
                                                    <td>Dirección:</td>
                                                    <td>bronce 1975</td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane fade p-3" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
                            <div class="d-flex">
                                <div class="row w-100">
                                    <div class="col-12">
                                        <div class="table table-responsive table-borderless">
                                            <table>
                                                <tr><p class="fw-bolder fs-5">Mis vacunas</p></tr>
                                                <tr>
                                                    <td>Triple Felina</td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane fade p-3" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
                            <p class="fw-bolder fs-5">Importante</p>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - QRMascotas - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>








